<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.incture.leaveme">


    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.ApplyLeavePage"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".activity.Notifications"></activity>
        <activity android:name=".activity.Reports"></activity>
        <activity android:name=".activity.Leave_history"></activity>
        <activity android:name=".Landing_page">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Holiday_Calendaar">
        </activity>
    </application>

</manifest>
